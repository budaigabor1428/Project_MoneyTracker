<h2>Edit <%= @merchant.name %></h2>
<form method="POST" action="/merchants/edit/<%= @merchant.id %>">
  <div class="form-wrapper">

    <div class="form-el">
      <label for="name">Name:</label>
      <input type="text" name="name" id="name" value="<%= @merchant.name %>"/>
    </div>

    <div class="form-el">
      <input type="submit" value="Edited"/>
    </div>

  </div>
</form>

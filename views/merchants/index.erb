
<h2>Merchants</h2>

<table>
  <tr>
    <th>Name</th>
  </tr>
  <% for merchant in @merchants %>
  <tr>
    <td><a href="/money-tracker/merchants/<%= merchant.id %>"><%= merchant.name %></a></td>
  </tr>
  <% end %>
</table>

<button type="button" name="button"><a href="/money-tracker/merchants/new">New merchant</a></button>

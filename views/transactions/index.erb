<h1>Transactions</h1>

<table>
  <tr>
  </tr>
  <% for transaction in @transactions %>
  <tr>
      <div class="transaction">
        <h3><a href="/money-tracker/transactions/<%= transaction.id %>"><%= transaction.amount %></a></h3>
        <p>Merchant: <%= transaction.merchant.name %></p>
        <p>Tag: <%= transaction.tag.name.capitalize %></p>
      </div>

  </tr>
  <% end %>
</table>
